#include <stdlib.h>
#include <time.h>
#include <stdio.h>
#include <ctype.h>
/**
 *main - Entry point
 *Return: Always 0 (Success)
 */
int main(void)
{
int ch;
char x = 'A';
while (x <= 'z')
{
x = tolower(x);
putchar(x);
x++;
}
ch = 'A';
while(ch <= 'Z')
{
ch = toupper(ch);
putchar(ch);
ch++;
}
putchar('\n');
return (0);
}
